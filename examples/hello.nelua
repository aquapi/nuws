local uws = require '../lib/main.nelua'

local app = uws.app()

app:any('/*', function(res: *uws.response, req: *uws.request): void
  print(req:method(), req:path())
  res:write_header('Cookie', 'a=b')
  res:write_header('Id', (@uint64)(12))
  res:finalize('Hello')
  end
)

app:listen(3000)
app:run()
